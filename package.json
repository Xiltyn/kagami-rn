{
	"name": "kagami-rn",
	"engines": {
		"node": "^10.16.3",
		"yarn": "^1.17.3"
	},
	"env": "stage",
	"main": "node_modules/expo/AppEntry.js",
	"scripts": {
		"start": "expo start -c",
		"dev": "REACT_DEBUGGER=\"unset ELECTRON_RUN_AS_NODE && open -g 'rndebugger://set-debugger-loc?port=19001' ||\" yarn run buildAndStart",
		"bundle:ios": "expo build:ios --release-channel $npm_package_env",
		"bundle:android": "expo build:android --release-channel $npm_package_env",
		"test": "node ./node_modules/jest/bin/jest.js --watchAll",
		"lint": "tslint --format stylish --project tsconfig.json",
		"build": "rimraf build && tsc --project tsconfig.json",
		"buildAndStart": "yarn run concurrently \"yarn run build -w\" \"yarn run start\""
	},
	"jest": {
		"preset": "jest-expo"
	},
	"dependencies": {
		"@expo/samples": "2.1.1",
		"@expo/vector-icons": "^10.0.0",
		"axios": "^0.18.0",
		"expo": "^34.0.3",
		"expo-asset": "^6.0.0",
		"expo-constants": "~6.0.0",
		"expo-font": "~6.0.1",
		"expo-location": "~6.0.0",
		"expo-vector-icons": "^10.0.1",
		"faker": "^4.1.0",
		"hex-rgba": "^1.0.2",
		"node-modules": "^1.0.1",
		"npm": "^6.10.3",
		"react": "16.8.3",
		"react-native": "https://github.com/expo/react-native/archive/sdk-34.0.0.tar.gz",
		"react-native-gesture-handler": "~1.3.0",
		"react-native-maps": "~0.24.0",
		"react-native-paper": "^2.16.0",
		"react-native-reanimated": "~1.1.0",
		"react-navigation": "^3.11.1",
		"react-navigation-tabs": "^2.3.0",
		"react-redux": "^6.0.0",
		"redux": "^4.0.1",
		"redux-actions": "^2.6.4",
		"redux-form": "^8.1.0",
		"redux-saga": "^1.0.5",
		"redux-thunk": "^2.3.0",
		"styled-components": "^4.3.2",
		"tslint-react-hooks": "^2.2.1"
	},
	"devDependencies": {
		"@babel/plugin-transform-typescript": "^7.5.2",
		"@types/faker": "^4.1.5",
		"@types/hex-rgba": "^1.0.0",
		"@types/jest": "^23.3.12",
		"@types/react": "^16.7.18",
		"@types/react-dom": "^16.0.11",
		"@types/react-navigation": "^3.0.8",
		"@types/react-redux": "^7.0.0",
		"@types/react-test-renderer": "^16.0.3",
		"@types/redux": "^3.6.0",
		"@types/redux-actions": "^2.3.1",
		"@types/redux-form": "^7.5.2",
		"@types/redux-thunk": "^2.1.0",
		"@types/styled-components": "^4.1.18",
		"babel-preset-expo": "^5.0.0",
		"concurrently": "^4.1.0",
		"jest-expo": "^34.0.0",
		"react-native-typescript-transformer": "^1.2.12",
		"redux-devtools-extension": "^2.13.8",
		"rimraf": "^2.6.3",
		"ts-jest": "^23.10.5",
		"tslint": "^5.12.1",
		"typescript": "^3.2.2"
	},
	"private": true
}
